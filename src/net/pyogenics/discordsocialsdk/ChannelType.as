package net.pyogenics.discordsocialsdk
{
    /**
     * Enum that represents the various channel types on Discord.
     * 
     * For more information see: https://discord.com/developers/docs/resources/channel
     */
    public class ChannelType
    {
        public static const GuildText:int = 0;
        public static const Dm:int = 1;
        public static const GuildVoice:int = 2;
        public static const GroupDm:int = 3;
        public static const GuildCategory:int = 4;
        public static const GuildNews:int = 5;
        public static const GuildStore:int = 6;
        public static const GuildNewsThread:int = 10;
        public static const GuildPublicThread:int = 11;
        public static const GuildPrivateThread:int = 12;
        public static const GuildStageVoice:int = 13;
        public static const GuildDirectory:int = 14;
        public static const GuildForum:int = 15;
        public static const GuildMedia:int = 16;
        public static const Lobby:int = 17;
        public static const EphemeralDm:int = 18;

    }
}