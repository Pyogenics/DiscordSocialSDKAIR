package net.pyogenics.discordsocialsdk
{
    /**
     * Represents the type of additional content contained in a message.
     */
    public class AdditionalContentType
    {
        public static const Other:int = 0;
        public static const Attachment:int = 1;
        public static const Poll:int = 2;
        public static const VoiceMessage:int = 3;
        public static const Thread:int = 4;
        public static const Embed:int = 5;
        public static const Sticker:int = 6;
    }
}